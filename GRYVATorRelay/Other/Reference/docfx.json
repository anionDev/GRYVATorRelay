{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "**.md",
          "**.yml"
        ],
        "src": "./ReferenceContent",
        "exclude": [
          "Examples/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "src": "./ReferenceContent"
      }
    ],
    "overwrite": [
      {
        "files": [],
        "exclude": []
      }
    ],
    "dest": "../Artifacts/Reference",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": true
  }
}